{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Elysia",
  "extends": "@tsconfig/bun/tsconfig.json",
  "compilerOptions": {
    "target": "ES2021",
    "module": "ES2022",
    "types": ["bun-types"],
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "jsx": "react"
  }
}
